<?xml version="1.0" encoding="utf-8"?>  
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 4.0//EN"  
        "http://tuckey.org/res/dtds/urlrewrite4.0.dtd">
<urlrewrite>
	<!-- 获取cms内容信息列表（分页） http://icms.goo.com/icms_p -->
	<rule>
		<from>^/icms_p$</from>
		<to>/icms.do?method=doService&amp;CType=cmsContentService&amp;MType=pageCmsContent</to>
	</rule>
	<!-- 获取cms内容信息列表（不分页） http://icms.goo.com/icms_l -->
	<rule>
		<from>^/icms_l$</from>
		<to>/icms.do?method=doService&amp;CType=cmsContentService&amp;MType=getCmsContentList</to>
	</rule>
	<!-- 获取cms内容详细 http://icms.goo.com/icms_de -->
	<rule>
		<from>^/icms_de$</from>
		<to>/icms.do?method=doService&amp;CType=cmsContentService&amp;MType=getCmsContent</to>
	</rule>
	<!-- 添加cms内容信息 http://icms.goo.com/icms_a -->
	<rule>
		<from>^/icms_a$</from>
		<to>/icms.do?method=doService&amp;CType=cmsContentService&amp;MType=add</to>
	</rule>
	<!-- 删除cms内容信息 http://icms.goo.com/icms_d -->
	<rule>
		<from>^/icms_d$</from>
		<to>/icms.do?method=doService&amp;CType=cmsContentService&amp;MType=delete</to>
	</rule>
	<!-- 修改cms内容信息 http://icms.goo.com/icms_u -->
	<rule>
		<from>^/icms_u$</from>
		<to>/icms.do?method=doService&amp;CType=cmsContentService&amp;MType=update</to>
	</rule>
	<!-- 审核cms内容信息 http://icms.goo.com/icms_c -->
	<rule>
		<from>^/icms_c$</from>
		<to>/icms.do?method=doService&amp;CType=cmsContentService&amp;MType=check</to>
	</rule>
	<!-- 发布cms内容信息 http://icms.goo.com/icms_r -->
	<rule>
		<from>^/icms_r$</from>
		<to>/icms.do?method=doService&amp;CType=cmsContentService&amp;MType=relese</to>
	</rule>
	<!-- 发布全部cms内容信息 http://icms.goo.com/icms_ra -->
	<rule>
		<from>^/icms_ra$</from>
		<to>/icms.do?method=doService&amp;CType=cmsContentService&amp;MType=releseAll</to>
	</rule>
	<!-- 初始化根结点 http://icms.goo.com/icms_i -->
	<rule>
		<from>^/icms_i$</from>
		<to>/icms.do?method=doService&amp;CType=cmsContentService&amp;MType=initRoot</to>
	</rule>
	<!-- 判断是否初初始化根结点 http://icms.goo.com/icms_is -->
	<rule>
		<from>^/icms_is$</from>
		<to>/icms.do?method=doService&amp;CType=cmsContentService&amp;MType=isInitRoot</to>
	</rule>
	<!--  栏目排序 http://icms.goo.com/icms_s -->
	<rule>
	    <from>^/icms_s$</from>
	    <to>/igms.do?method=doService&amp;CType=cmsContentService&amp;MType=sort</to>
	</rule>
	<!-- 模板列表 http://icms.goo.com/icms_tl -->
	<rule>
		<from>^/icms_tl$</from>
		<to>/icms.do?method=doService&amp;CType=templateInfoService&amp;MType=getTemplateInfoList</to>
	</rule>
	<!--  模板详细信息细 http://icms.goo.com/icms_td -->
	<rule>
	    <from>^/icms_td$</from>
	    <to>/igms.do?method=doService&amp;CType=templateInfoService&amp;MType=getTemplateInfo</to>
	</rule>
</urlrewrite>  