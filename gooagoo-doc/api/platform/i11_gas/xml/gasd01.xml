<?xml version="1.0" encoding="utf-8" ?>
<root alias="CategoryRoot"><!--商家的品类查询 -->
	<result></result><!--查询结果编码，true-成功，false-失败 -->
	<msg></msg><!--查询结果描述 -->
	<msgcode></msgcode><!-- 提示信息编号 -->
	<categoryinfo cycle="1"><!--品类信息 -->
		<categoryid></categoryid><!--分类id -->
		<categoryname></categoryname><!--分类名称 -->
		<categoryurl/><!--分类图片url -->
		<categorysub cycle="1"><!--子分类 -->
			<categorysubid></categorysubid><!--子分类id -->
			<categorysubname></categorysubname><!--子分类名称 -->
			<categorysuburl/><!--子分类图片url -->
		</categorysub>
	</categoryinfo>
</root>

