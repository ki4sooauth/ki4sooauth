<?xml version="1.0" encoding="UTF-8"?>
<root alias="DeskQueueTrackRoot"><!--获取排号记录-->
	<result/><!--结果编码，true获取排号记录成功，false获取排号记录失败-->
	<msg/><!--结果描述-->
	<msgcode></msgcode><!-- 提示信息编号 -->
	<deskqueuetrack cycle="1"><!-- 各类型餐桌排号记录 -->
		<deskkind></deskkind><!--餐桌类型,如：1-2人桌 -->
		<queuenos></queuenos><!--当前餐桌类型等待排队的排队号码,以json形式保存,如["1","2"] -->
	</deskqueuetrack>
 </root>