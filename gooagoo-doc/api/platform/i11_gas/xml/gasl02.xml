<?xml version="1.0" encoding="utf-8" ?>
<root alias="GetAllBillRoot"><!--查看全部订单-->
	<result></result><!--查询结果编码，true-查询成功，false-查询失败-->
	<msg></msg><!--查询结果描述-->
	<msgcode></msgcode><!-- 提示信息编号 -->
	<desklist cycle="1">
		<deskno></deskno><!-- 餐桌号 -->
		<desktypecode></desktypecode><!--餐桌类型编码  -->
		<desktypename></desktypename><!--餐桌类型名称  -->
		<isvip></isvip><!--餐桌上客户类型 0-会员 1-非会员 -->
		<scardno></scardno><!-- 会员卡音频编号 -->
		<deskstate></deskstate><!--餐桌状态:0-已开台 1-空闲, 2-已点餐  ，3正在结账-->
		<userordertime></userordertime><!-- 用户提交订单时间 -->
		<mealtime></mealtime><!--用户用餐时长 ,返回时间格式为: HH:mm:ss-->
	</desklist>
</root>

