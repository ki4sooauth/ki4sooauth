	<msg></msg><!--查询结果描述-->
	<msgcode></msgcode><!-- 提示信息编号 -->
	<discountgoods cycle="1"><!--品类优惠信息-->
		<shopid></shopid><!--商家id-->
		<goodsid></goodsid><!--商品id-->
		<goodsname></goodsname><!--商品名称-->
		<price></price><!--商品价格-->
		<discountprice></discountprice><!--商品价格，实体店价格-->
		<goodsimg></goodsimg><!--商品多个展示图片,以|隔开-->
		<goodscategoryleaf></goodscategoryleaf><!--品类名称-->
		<goodsbrand></goodsbrand><!--品牌名称-->
		<goodsdetailextend cycle="1"><!--产品特征-->
			<featurecode></featurecode><!--特征编码-->
			<featurename></featurename><!--特征名称-->
			<featurevalue></featurevalue><!--特征数值-->
		</goodsdetailextend>
	</discountgoods>
</root>

