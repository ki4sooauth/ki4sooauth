<?xml version="1.0" encoding="utf-8" ?>
<root alias="UserFavRoot"><!--TA的收藏商品查询 -->
	<result></result><!--查询结果编码，true-查询成功，false-查询失败 -->
	<msg></msg><!--查询结果描述 -->
	<msgcode></msgcode><!-- 提示信息编号 -->
	<userFavInfo cycle="1"><!--TA的收藏商品信息 -->
		<shopid></shopid><!--店铺id -->
		<goodsid></goodsid><!--商品id -->
		<goodsname></goodsname><!--商品名称 -->
		<goodsprice></goodsprice><!--商品价格 -->
		<goodsimg></goodsimg><!--商品多个展示图片,以，隔开 -->
		<goodscategorylistf cycle="1"><!--商品类别,查出该商品对应的根节点到叶节点的所有类别 -->
			<categoryid></categoryid><!--品类编号 -->
			<categoryname></categoryname><!--品类名称 -->
		</goodscategorylistf>
		<goodsbrand></goodsbrand><!--品牌名称 -->
	</userFavInfo>
</root>