<?xml version="1.0" encoding="UTF-8"?>
<root alias="GetPositionRoot"><!--位置布局-->
	<result></result><!--查询结果编码，true-成功，false-失败-->
	<msg></msg><!--查询结果描述-->
	<msgcode></msgcode><!-- 提示信息编号 -->
	<positionInfo cycle="1"><!--位置信息-->
		<positionId></positionId><!--位置编号，唯一，通过UUID产生-->
		<positionName></positionName><!--位置名称-->
		<shopId></shopId><!--商家编号-->
		<shopEntityId></shopEntityId><!--实体店编号-->
		<parentPositionId></parentPositionId><!--父位置编号-->
		<parentPositionName></parentPositionName><!--父级名称-->
		<description></description><!--位置描述-->
		<isDel></isDel><!--是否删除-->
		<createTime></createTime><!--创建时间-->
		<cTimeStamp></cTimeStamp><!--最后一次修改时间-->
	</positionInfo>
</root>