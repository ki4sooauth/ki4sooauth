<?xml version="1.0" encoding="UTF-8"?>
<root alias="DeskStatusDetailRoot"><!--排队-->
	<result/><!--结果编码，true成功，false失败-->
	<msg/><!--结果描述-->
	<msgcode></msgcode><!-- 提示信息编号 -->
	<desklist cycle="1"><!-- 餐桌状态列表 -->
		<tabletypecode></tabletypecode><!--餐桌类型 -->
		<isvip></isvip><!--餐桌上客户类型 0-会员 1-非会员 -->
		<tablename></tablename><!-- 餐桌名称-->
		<tablestate></tablestate><!-- 餐桌状态:0-已开台 1-空闲, 2-已点餐  ,3-正在结账-->
		<orderid></orderid><!-- 订单编号-->
		<mealtime></mealtime><!--用户用餐时长,返回时间格式为: HH:mm:ss空闲显示00：00：00 -->
		<scardno></scardno><!--如果是会员，则表示会员卡号-->
		<userordertime></userordertime><!--HH:MM:SS用户提交订单时间-->
		<goodsnums></goodsnums><!--商品总数-->
		<ischecknum></ischecknum><!-- 已上菜品数 -->
		<notchecknum></notchecknum><!--未上菜品数 -->
	</desklist>
 </root>