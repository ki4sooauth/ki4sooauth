<?xml version="1.0" encoding="UTF-8"?>
<root alias="deskStatusRoot"><!--餐桌状态-->
	 <result/><!--结果编码，true成功，false失败-->
	 <msg/><!--结果描述-->
	 <msgcode></msgcode><!-- 提示信息编号 -->
	 <deskstatuslist cycle="1"><!--餐桌状态-->
		<sum></sum><!-- 当前类型餐桌总数，如:3-8人桌总数 -->
		<tabletypecode></tabletypecode><!-- 餐桌类型编码 -->
		<tabletypename></tabletypename><!-- 餐桌类型名称 -->
	 	<free></free><!--未被占有桌子数，即空余数-->
	 	<occupancy></occupancy><!--正在结账数-->
	 	<usenum></usenum><!--正在用餐数量,即已点餐数-->
	 	<list></list><!--共有多少人排队等待使用当前类型的桌子-->
	 </deskstatuslist> 
 </root>