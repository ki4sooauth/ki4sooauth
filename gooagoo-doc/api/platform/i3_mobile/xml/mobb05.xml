<?xml version="1.0" encoding="UTF-8"?>
<root alias="UserCommentRoot"><!-- 获取商品所有评论 -->
	<result></result><!--查询结果编码，true-成功，false-失败 -->
	<msg></msg><!--查询失败描述 -->
	<msgcode></msgcode><!-- 提示信息编号 -->
	<totalrec></totalrec><!--总评论数目 -->
	<usercomment cycle="1"><!-- 评论信息 -->
		<commentid></commentid><!--评论的id -->
		<nickname></nickname><!--评论人的昵称 -->
		<description></description><!--商品概要描述 -->
		<commentlevel></commentlevel><!--商品评分 -->
		<content></content><!--评论的内容 -->
		<source></source><!--评论来源：M-手机端按机型分类，P-网站端按浏览器分类 -->
		<createtime></createtime><!--评论时间 -->
	</usercomment>
</root>