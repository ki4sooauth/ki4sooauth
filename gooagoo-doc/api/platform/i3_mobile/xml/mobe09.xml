<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<root alias="GoodsInfoFromOnecodevalueRoot"><!--通过一维码获取商品信息-->
	<result></result><!--查询结果编码，true-成功，false-失败 -->
	<msg></msg><!--查询失败描述 -->
	<msgcode></msgcode><!-- 提示信息编号 -->
	<goodscontaincomment cycle="1"><!--商品信息和评论  -->
		<onedimensioncode><!--商品信息 -->
			<goodsid></goodsid><!--商品id -->
			<goodsserial></goodsserial><!-- 商品序列号（商品的唯一识别编码） -->
			<itemserial></itemserial><!-- 自定义序列号（商品细分的唯一识别编码） -->
			<shopid></shopid><!--商家id -->
			<goodsname></goodsname><!--商品名称 -->
			<price></price><!--单价-->
			<goodscontent></goodscontent><!--描述 -->
			<goodsimg></goodsimg><!--图片（小图） -->
			<commentnums></commentnums><!--评论次数 goods_statistic_info.comment_nums-->
			<goodsscore></goodsscore><!--商品评分goods_statistic_info.goods_score -->
			<introduceurl></introduceurl><!-- 商品详情url,商品详情介绍url -->
		</onedimensioncode>
		<usercomment cycle="1"><!--商品评论，目前取3條 -->
			<commentlevel></commentlevel><!--评分user_comment.comment_level-->
			<content></content><!--评论的具体内容 user_comment.content-->
			<createtime></createtime><!--创建时间(评论时间)user_comment.create_time -->
			<nickname></nickname><!--昵称user_info.nick_name -->
		</usercomment>
	</goodscontaincomment>
</root>