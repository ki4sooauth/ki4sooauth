<?xml version="1.0" encoding="utf-8" ?>
<root alias="CryoutListgRoot"><!-- 吆喝广场分类接口 -->
	<result></result><!--查询结果编码，true-成功，false-失败 -->
	<msg></msg><!--查询失败描述 -->
	<msgcode></msgcode><!-- 提示信息编号 -->
	<cryoutlistg cycle="1"><!-- 一级菜单 -->
		<cryoutid></cryoutid><!--吆喝的id -->
		<pageid></pageid><!--分页查询编号：吆喝推送时间（YYYY-MM-DD HH:MM:SS）+吆喝编号 -->
		<cryoutuserid></cryoutuserid><!--吆喝和用户关联表的UUID -->
		<userid></userid><!--用户id -->
		<shopid></shopid><!--商家的id -->
		<shopname></shopname><!--商家名称 -->
		<logo></logo><!--商家的logo -->
		<address></address><!--商家地址 -->
		<cryouttextmobile></cryouttextmobile><!--吆喝的内容（有特殊格式的规定） -->
		<thumbnailpic></thumbnailpic><!--极小的缩略图 -->
		<bmiddlepic></bmiddlepic><!--大点的图 -->
		<originalpic></originalpic><!--原始图片（最大的） -->
		<source></source><!--信息来源：L-现场，R-远程 -->
		<createtime></createtime><!--吆喝创建时间 -->
		<allowat></allowat><!--是否允许关注：Y-允许，N-不允许  -->
		<relation></relation><!--是否已经关注或者是会员：0-关注卡，1-电子卡，2-实体卡，N-未关注或会员 -->
	</cryoutlistg>
	<isdeleted><!--平台已删除的吆喝吆喝编号信息 -->
		<cryoutidstr></cryoutidstr><!--，字符串，已删除吆喝的编号，多个用逗号分隔开 -->
		<flag></flag><!--返回吆喝信息的总条数是否大于每页信息显示条数，若大于flag=Y，反之Flag=N -->
		<ctimestamp></ctimestamp><!-- 记录平台返回吆喝信息中 最大的时间戳 -->
	</isdeleted>	
</root>