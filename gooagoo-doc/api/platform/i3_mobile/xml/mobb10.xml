<?xml version="1.0" encoding="UTF-8"?>
<root alias="FavoriteRecommendRoot"><!-- 搜索收藏推荐 -->
	<result></result><!--查询结果编码，true-成功，false-失败 -->
	<msg></msg><!--查询失败描述 -->
	<msgcode></msgcode><!-- 提示信息编号 -->
	<favoriterecommend cycle="1"><!-- 收藏信息 -->
		<shopid></shopid><!--店铺的id -->
		<shopname></shopname><!--店铺名称 -->
		<logo></logo><!--店铺的logo -->
		<infourl></infourl><!--相应信息的网址（如活动） -->
		<infotype></infotype><!--信息的类型：现在有商品、活动、优惠劵 -->
		<remark></remark><!--备注描述信息,暂未使用 -->
		<infotitle></infotitle><!--信息标题，如商品的名称 -->
		<infobegindate></infobegindate><!--信息开始日期，优惠和活动的期间 -->
		<infoenddate></infoenddate><!--信息结束日期 -->
		<price></price><!--商品价格 -->
		<isused></isused><!--优惠是否已使用 -->
		<infopic></infopic><!--信息的小图片 -->
		<isdel></isdel><!-- 是否删除，Y-已删除，N-未删除 -->
		<ctimestamp></ctimestamp><!--时间戳 -->
	</favoriterecommend>
</root>