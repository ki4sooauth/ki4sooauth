<?xml version="1.0" encoding="UTF-8"?>
<root alias="deskStatusRoot"><!-- 餐桌状态查询 -->
	<result></result><!--查询结果编码，true-成功，false-失败 -->
	<msg></msg><!--查询失败描述 -->
	<msgcode></msgcode><!-- 提示信息编号 -->
	<deskstatuslist cycle="1"><!--餐桌状态查询 -->
		<desktype></desktype><!--餐桌类型（几人桌） -->
		<minnums></minnums><!--建议最小人数 -->
		<maxnums></maxnums><!--建议最大人数 -->
		<totalnum></totalnum><!--该类型餐桌总数 -->
		<vacancynum></vacancynum><!--该类型餐桌空闲数 -->
		<checkoutnum></checkoutnum><!--该类型餐桌正在结帐数 -->
		<queuenum></queuenum><!--该类型餐桌排队号数（前面有多少号） -->
	</deskstatuslist>
</root>