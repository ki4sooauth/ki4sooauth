<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<root alias="GetOrderFormRoot"><!--通过桌号获取点菜单信息 -->
	<result></result><!--查询结果编码，true-成功，false-失败 -->
	<msg></msg><!--查询失败描述 -->
	<msgcode></msgcode><!-- 提示信息编号 -->
	<getorderform><!--订单 -->
		<billid></billid><!--点菜单id -->
		<shopid></shopid><!--商家id -->
		<userid></userid><!--用户id -->
		<scardno></scardno><!--用户会员卡id -->
		<discountrate></discountrate><!--打折率 -->
		<goodstotalnum></goodstotalnum><!--商品累计数量 -->
		<originalprice></originalprice><!--原始价格总数 -->
		<payprice></payprice><!--实收总额 -->
		<shopname></shopname><!--商家名称 -->
		<logo></logo><!--商家logo -->
		<billimg></billimg><!--点菜单图片路径 -->
		<reserve2></reserve2><!--桌号 -->
		<reserve1></reserve1><!--房间号 -->
		<goodslist cycle="1"><!--商品信息列表 -->
			<goodsid></goodsid><!--商品id -->
			<goodsname></goodsname><!--商品名称 -->
			<goodsprice></goodsprice><!--商品价格 -->
			<goodsimg></goodsimg><!--商品图片地址 -->
			<promotionprice></promotionprice><!--促销价格实际支付价格 -->
			<goodsnum></goodsnum><!--数量 -->
			<introduceurl></introduceurl><!-- 商品详情url,商品详情介绍url -->
		</goodslist>
	</getorderform>
</root>