<?xml version="1.0" encoding="UTF-8"?>
<root alias="MembernoticeRoot"><!-- 用户得到"通知" -->
	<result></result><!--查询结果编码，true-成功，false-失败 -->
	<msg></msg><!--查询失败描述 -->
	<msgcode></msgcode><!-- 提示信息编号 -->
	<membernotice cycle="1"><!--通知 -->
		<isread></isread><!--是否已读 -->
		<noticeinfoid></noticeinfoid><!--通知编号 -->
		<pageid></pageid><!--分页编号:通知推送时间（YYYY-MM-DD HH:MM:SS） +通知编号-->
		<noticeuserid></noticeuserid><!--通知和用户关联表的UUID -->
		<shopid></shopid><!--店铺id -->
		<logo></logo><!--店铺logo -->
		<title></title><!--通知名称 -->
		<img></img><!--小图片 -->
		<noticetextmobile></noticetextmobile><!--通知详情 -->
		<ctimestamp></ctimestamp><!--提交时间 -->
		<isdel></isdel><!--是否删除 -->
	</membernotice>
	<isdeleted><!--平台已删除的通知编号信息 -->
		<noticeidstr></noticeidstr><!--，字符串，已删除通知的编号，多个用逗号分隔开 -->
		<flag></flag><!--返回通知的总条数是否大于每页信息显示条数，若大于flag=Y，反之Flag=N -->
		<ctimestamp></ctimestamp><!-- 记录平台返回通知信息中 最大的时间戳 -->
	</isdeleted>
</root>