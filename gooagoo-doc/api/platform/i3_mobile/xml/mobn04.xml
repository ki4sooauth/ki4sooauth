<?xml version="1.0" encoding="UTF-8"?>
<root alias="QueryGoodsOfShoppingCartRoot"><!--查询购物车中的商品 -->
	<result></result><!--查询结果编码，true-成功，false-失败 -->
	<msg></msg><!--结果描述 -->
	<msgcode></msgcode><!-- 提示信息编号 -->
	<goodsinfolist cycle="1"><!-- 购物车中商品信息 -->
	    <shoppingcartid></shoppingcartid><!--购物车编号, 自动编号，UUID,购物车表主键 -->
		<shopid></shopid><!-- 商家编号 -->
		<shopenityid></shopenityid><!-- 实体店编号 -->
		<goodsid></goodsid><!-- 商品编号 -->
		<goodsname></goodsname><!-- 商品名称 -->
		<itemserial></itemserial><!-- 商品自定义序列号 -->
		<price></price><!-- 商品价格 -->
		<goodsimg></goodsimg><!-- 商品主图地址 -->
		<goodsnum></goodsnum><!-- 商品数量 -->
		<introduceurl></introduceurl><!-- 商品详情url,商品详情介绍url -->
		<goodsmarketingurl></goodsmarketingurl><!-- 商品营销url -->
	</goodsinfolist>
</root>