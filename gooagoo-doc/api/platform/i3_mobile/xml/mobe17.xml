<?xml version="1.0" encoding="UTF-8"?>
<root alias="refreshQueueRoot"><!-- 刷新排号  -->
	<result></result><!--查询结果编码，true-成功，false-失败 -->
	<msg></msg><!--查询失败描述 -->
	<msgcode></msgcode><!-- 提示信息编号 -->
	<refreshqueue cycle="1"><!-- 刷新排号 -->
		<shopid></shopid><!-- 商家编号 -->
		<shopentityid></shopentityid><!-- 实体店编号 -->
		<queueno></queueno><!--排队号码-->
		<currentqueueno></currentqueueno><!--当前叫到排队号码 -->
		<waitnum></waitnum><!--排队等候号数 -->
		<checkoutnum></checkoutnum><!--正在结帐数-->
		<createtime></createtime><!--排号时间-->
	</refreshqueue>
</root>