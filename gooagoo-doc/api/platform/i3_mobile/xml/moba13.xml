<?xml version="1.0" encoding="UTF-8"?>
<root alias="GetMemberBaseInfoRoot"><!-- 查询会员信息 -->
	<result></result><!--查询结果编码，true-成功，false-失败 -->
	<msg></msg><!--查询失败描述 -->
	<msgcode></msgcode><!-- 提示信息编号 -->
	<memberbaseinfo>
		<name></name><!--姓名 -->
		<sex></sex><!--性别，M-男，F-女 -->
		<birthday></birthday><!--出生日期 -->
		<idtype></idtype><!--证件类型，00-身份证，01-护照，02-军官证，03-其他 -->
		<idno></idno><!--证件号码 -->
		<mobile></mobile><!--手机号码 -->
		<telephone></telephone><!--联系电话 -->
		<email></email><!--电子邮箱 -->
		<postcode></postcode><!--邮政编码 -->
		<address></address><!--通讯地址 -->
		<memberspecialinfo cycle="1">
			<typecode></typecode><!--类型编号，例如color，同一商家唯一，由商家录入-->
			<typename></typename><!--类型名称，例如颜色，同一商家唯一，由商家录入-->
			<enumvalue></enumvalue><!--枚举值，保存的是json串，例如["黄色","蓝色","白色"]-->
			<featurevalue></featurevalue><!--特征数值（用户当前的数值），如蓝色 -->
		</memberspecialinfo>
	</memberbaseinfo>
</root>