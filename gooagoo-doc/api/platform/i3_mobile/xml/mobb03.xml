<?xml version="1.0" encoding="UTF-8"?>
<root alias="VoucherDetailRoot"><!-- 获取优惠劵信息 -->
	<result></result><!--查询结果编码，true-成功，false-失败 -->
	<msg></msg><!--查询失败描述 -->
	<msgcode></msgcode><!-- 提示信息编号 -->
	<voucherdetail cycle="1"><!-- 优惠劵信息 -->
		<shopid></shopid><!--店铺id -->
		<couponuserid></couponuserid><!--优惠凭证与用户关联编号，UUID-->
		<couponname></couponname><!--优惠劵名称 -->
		<couponurl></couponurl><!--优惠劵正面图 -->
		<publishstarttime></publishstarttime><!--优惠劵开始时间 -->
		<publishendtime></publishendtime><!--优惠劵结束时间 -->
		<couponcontent></couponcontent><!--优惠劵权限说明 -->
		<coupontype></coupontype><!--优惠凭证类型，参考通用字典表的coupon_type -->
		<couponvalue></couponvalue><!--优惠券数值 -->
	</voucherdetail>
</root>