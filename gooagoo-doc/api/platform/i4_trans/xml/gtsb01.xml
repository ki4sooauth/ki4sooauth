<?xml version="1.0" encoding="UTF-8"?>
<root alias="SynchronousMemberCardRoot"><!--需要同步的会员卡和用户关联信息查询-->
	<result></result><!--结果编码，true-查询成功，false-查询失败-->
	<msg></msg><!--结果描述-->
	<msgcode></msgcode><!-- 提示信息编号 -->
    <memberofcard cycle="1"><!--会员信息-->
		<scardno></scardno><!--会员卡音频编号-->
		<userid></userid><!--用户编号-->
		<cardtype2></cardtype2><!--会员卡类型，00表示关注信息卡，10-F0表示电子卡，F1-FF表示实体卡-->
		<shopid></shopid><!--商铺id-->
		<phycardno></phycardno><!--物理卡号-->
		<createtime></createtime><!--有效期限起-->
		<expiredate></expiredate><!--有效期限止-->
		<ctimestamp></ctimestamp><!--时间戳-->
		<isdel></isdel><!--是否删除，Y-已删除，N-未删除-->
	</memberofcard>
</root>