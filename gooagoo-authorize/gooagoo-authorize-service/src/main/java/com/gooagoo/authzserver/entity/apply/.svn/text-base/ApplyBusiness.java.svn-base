package com.gooagoo.authzserver.entity.apply;

import java.io.Serializable;

/**
 * client 信息
 * @author Administrator
 *
 */
public class ApplyBusiness implements Serializable
{
    private static final long serialVersionUID = 1L;

    public String id;
    /**
     * 申请应用时网站分配的clientId
     */
    public String clientId;
    /**
     * 申请应用时网站分配的clientSecret
     */
    public String clientSecret;
    /**
     * 临时token 
     */
    public String code;
    /**
     * 访问token
     */
    public String accessToken;
    /**
     * code开始时间
     */
    public String codeStartTime;
    /**
     * code结束时间
     */
    public String codeEndTime;
    /**
     * token开始时间
     */
    public String tokenStartTime;
    /**
     * token结束时间
     */
    public String tokenEndTime;
    /**
     * 可进行授权的列表 
     */
    public String scope;

    public String getId()
    {
        return this.id;
    }

    public void setId(String id)
    {
        this.id = id;
    }

    public String getClientId()
    {
        return this.clientId;
    }

    public void setClientId(String clientId)
    {
        this.clientId = clientId;
    }

    public String getClientSecret()
    {
        return this.clientSecret;
    }

    public void setClientSecret(String clientSecret)
    {
        this.clientSecret = clientSecret;
    }

    public String getCode()
    {
        return this.code;
    }

    public void setCode(String code)
    {
        this.code = code;
    }

    public String getAccessToken()
    {
        return this.accessToken;
    }

    public void setAccessToken(String accessToken)
    {
        this.accessToken = accessToken;
    }

    public String getCodeStartTime()
    {
        return this.codeStartTime;
    }

    public void setCodeStartTime(String codeStartTime)
    {
        this.codeStartTime = codeStartTime;
    }

    public String getCodeEndTime()
    {
        return this.codeEndTime;
    }

    public void setCodeEndTime(String codeEndTime)
    {
        this.codeEndTime = codeEndTime;
    }

    public String getTokenStartTime()
    {
        return this.tokenStartTime;
    }

    public void setTokenStartTime(String tokenStartTime)
    {
        this.tokenStartTime = tokenStartTime;
    }

    public String getTokenEndTime()
    {
        return this.tokenEndTime;
    }

    public void setTokenEndTime(String tokenEndTime)
    {
        this.tokenEndTime = tokenEndTime;
    }

    public String getScope()
    {
        return this.scope;
    }

    public void setScope(String scope)
    {
        this.scope = scope;
    }

    @Override
    public String toString()
    {
        return "ApplyBusiness [id=" + this.id + ", clientId=" + this.clientId + ", clientSecret=" + this.clientSecret + ", code=" + this.code + ", accessToken=" + this.accessToken + ", codeStartTime=" + this.codeStartTime + ", codeEndTime=" + this.codeEndTime + ", tokenStartTime=" + this.tokenStartTime + ", tokenEndTime=" + this.tokenEndTime + ", scope=" + this.scope + "]";
    }
}
