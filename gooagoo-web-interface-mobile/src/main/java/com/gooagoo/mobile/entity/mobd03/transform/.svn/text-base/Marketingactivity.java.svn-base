package com.gooagoo.mobile.entity.mobd03.transform;

import java.io.Serializable;

/**
 *  活动查询 
 */
public class Marketingactivity implements Serializable
{

	private static final long serialVersionUID = 1L;

	/**  活动类型（活动包含哪些类型），根据发布活动的商家类型区分。1-餐饮；a-非餐饮。格式：以逗号分隔的字符串，示例：1,a  */
	private String type = "";

	/** 时间（月（1-12）、日（1-31））  */
	private String date = "";

	/**
	 * 设置 活动类型（活动包含哪些类型），根据发布活动的商家类型区分。1-餐饮；a-非餐饮。格式：以逗号分隔的字符串，示例：1,a 
	 * @param type	 活动类型（活动包含哪些类型），根据发布活动的商家类型区分。1-餐饮；a-非餐饮。格式：以逗号分隔的字符串，示例：1,a 
	 */
	public void setType(String type)
	{
		this.type = type != null ? type : "";
	}

	/**
	 * 获取 活动类型（活动包含哪些类型），根据发布活动的商家类型区分。1-餐饮；a-非餐饮。格式：以逗号分隔的字符串，示例：1,a 
	 * @return	 活动类型（活动包含哪些类型），根据发布活动的商家类型区分。1-餐饮；a-非餐饮。格式：以逗号分隔的字符串，示例：1,a 
	 */
	public String getType()
	{
		return this.type;
	}

	/**
	 * 设置时间（月（1-12）、日（1-31）） 
	 * @param date	时间（月（1-12）、日（1-31）） 
	 */
	public void setDate(String date)
	{
		this.date = date != null ? date : "";
	}

	/**
	 * 获取时间（月（1-12）、日（1-31）） 
	 * @return	时间（月（1-12）、日（1-31）） 
	 */
	public String getDate()
	{
		return this.date;
	}
}