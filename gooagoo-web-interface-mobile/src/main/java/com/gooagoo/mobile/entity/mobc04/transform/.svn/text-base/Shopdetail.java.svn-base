package com.gooagoo.mobile.entity.mobc04.transform;

import java.io.Serializable;

/**
 * 吆喝广场商家查询（输入参数：商家id和用户id 
 */
public class Shopdetail implements Serializable
{

	private static final long serialVersionUID = 1L;

	/** 商家id  */
	private String shopid = "";

	/** 商家logo1，正方形  */
	private String logo1 = "";

	/** 商家logo2，长方形  */
	private String logo2 = "";

	/** 会员卡卡头url  */
	private String cardheaderurl = "";

	/** 关注数量  */
	private String attentionnum = "";

	/** 会员数量  */
	private String membernum = "";

	/**
	 * 设置商家id 
	 * @param shopid	商家id 
	 */
	public void setShopid(String shopid)
	{
		this.shopid = shopid != null ? shopid : "";
	}

	/**
	 * 获取商家id 
	 * @return	商家id 
	 */
	public String getShopid()
	{
		return this.shopid;
	}

	/**
	 * 设置商家logo1，正方形 
	 * @param logo1	商家logo1，正方形 
	 */
	public void setLogo1(String logo1)
	{
		this.logo1 = logo1 != null ? logo1 : "";
	}

	/**
	 * 获取商家logo1，正方形 
	 * @return	商家logo1，正方形 
	 */
	public String getLogo1()
	{
		return this.logo1;
	}

	/**
	 * 设置商家logo2，长方形 
	 * @param logo2	商家logo2，长方形 
	 */
	public void setLogo2(String logo2)
	{
		this.logo2 = logo2 != null ? logo2 : "";
	}

	/**
	 * 获取商家logo2，长方形 
	 * @return	商家logo2，长方形 
	 */
	public String getLogo2()
	{
		return this.logo2;
	}

	/**
	 * 设置会员卡卡头url 
	 * @param cardheaderurl	会员卡卡头url 
	 */
	public void setCardheaderurl(String cardheaderurl)
	{
		this.cardheaderurl = cardheaderurl != null ? cardheaderurl : "";
	}

	/**
	 * 获取会员卡卡头url 
	 * @return	会员卡卡头url 
	 */
	public String getCardheaderurl()
	{
		return this.cardheaderurl;
	}

	/**
	 * 设置关注数量 
	 * @param attentionnum	关注数量 
	 */
	public void setAttentionnum(String attentionnum)
	{
		this.attentionnum = attentionnum != null ? attentionnum : "";
	}

	/**
	 * 获取关注数量 
	 * @return	关注数量 
	 */
	public String getAttentionnum()
	{
		return this.attentionnum;
	}

	/**
	 * 设置会员数量 
	 * @param membernum	会员数量 
	 */
	public void setMembernum(String membernum)
	{
		this.membernum = membernum != null ? membernum : "";
	}

	/**
	 * 获取会员数量 
	 * @return	会员数量 
	 */
	public String getMembernum()
	{
		return this.membernum;
	}
}