    <result column="channel_type" property="channelType" jdbcType="CHAR" />
    <result column="shop_id" property="shopId" jdbcType="CHAR" />
    <result column="cms_content_name" property="cmsContentName" jdbcType="CHAR" />
    <result column="cms_content_img" property="cmsContentImg" jdbcType="CHAR" />
    <result column="cms_content_des" property="cmsContentDes" jdbcType="CHAR" />
    <result column="cms_content_url" property="cmsContentUrl" jdbcType="VARCHAR" />
