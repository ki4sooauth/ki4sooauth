    <result column="starting_price" property="startingPrice" jdbcType="DOUBLE" />
    <result column="increase" property="increase" jdbcType="DOUBLE" />
    <result column="state" property="state" jdbcType="CHAR" />
    <result column="winner_shoo_id" property="winnerShooId" jdbcType="CHAR" />
    <result column="winner_shoo_name" property="winnerShooName" jdbcType="CHAR" />
    <result column="bid_amount" property="bidAmount" jdbcType="DOUBLE" />
    <result column="id" property="id" jdbcType="CHAR" />
    <result column="bid_start_time" property="bidStartTime" jdbcType="TIMESTAMP" />
