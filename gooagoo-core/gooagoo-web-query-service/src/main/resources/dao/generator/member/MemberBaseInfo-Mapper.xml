    </if>
  </select>

  <!-- 按对象查询数据条数 -->
  <select id="countByExample" parameterType="com.gooagoo.entity.generator.member.MemberBaseInfoExample" resultType="java.lang.Integer">
    select count(*) from member_base_info
    <if test="_parameter != null">
       <include refid="Example_Where_Clause" />
