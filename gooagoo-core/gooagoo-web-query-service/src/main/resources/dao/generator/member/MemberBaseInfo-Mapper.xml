  </select>

  <!-- 按主键查询表信息 -->
  <select id="selectByPrimaryKey" resultMap="BaseResultMap" parameterType="com.gooagoo.entity.generator.member.MemberBaseInfoKey" >
    select 
      id, shop_id, phy_no, 
      phy_name, name, sex, 
      birthday, id_type, id_no, 
      mobile, telephone, email, 
      postcode, address, is_del, 
      create_time, c_time_stamp
    from member_base_info where id = #{id,jdbcType=CHAR}
    <if test= "isDel != null">
         and is_del=#{isDel}
    </if>
  </select>

