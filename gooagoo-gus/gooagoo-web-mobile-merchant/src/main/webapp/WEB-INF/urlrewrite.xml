<?xml version="1.0" encoding="utf-8"?>  
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 4.0//EN"  
        "http://tuckey.org/res/dtds/urlrewrite4.0.dtd">
<urlrewrite>
	<rule>
		<from>^/$</from>
		<to>/merchant.do?method=index</to>
	</rule>
	<rule>
		<from>^/info$</from>
		<to>/merchant.do?method=index</to>
	</rule>
	<rule>
		<from>^/apk$</from>
		<to>/apk.do?method=download</to>
	</rule>
	<rule>
		<from>^/apk2$</from>
		<to>/apk.do?method=index</to>
	</rule>
</urlrewrite>  