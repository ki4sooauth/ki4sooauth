<?xml version="1.0" encoding="utf-8"?>  
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 4.0//EN"  
        "http://tuckey.org/res/dtds/urlrewrite4.0.dtd">
<urlrewrite>
	<rule>
		<from>^/$</from>
		<to>/personal.do?method=index</to>
	</rule>
	<rule>
		<from>^/index$</from>
		<to>/personal.do?method=index</to>
	</rule>
	<rule>
		<from>^/userBoundEmail$</from>
		<to>/email.do?method=activeEmail</to>
	</rule>
	<rule>
		<from>^/userIdentityForBoundMobile$</from>
		<to>/mobile.do?method=activeMobile</to>
	</rule>
	<rule>
		<from>^/setAccount$</from>
		<to>/account.do?method=show</to>
	</rule>
</urlrewrite>  