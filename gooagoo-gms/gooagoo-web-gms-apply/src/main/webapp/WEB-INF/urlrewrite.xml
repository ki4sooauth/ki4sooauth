<?xml version="1.0" encoding="utf-8"?>  
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 4.0//EN"  
        "http://tuckey.org/res/dtds/urlrewrite4.0.dtd">  
<urlrewrite>  
<!-- 跳转到子节点 开始-->
    <rule>  
        <from>^/$</from>  
        <to>/common.do?method=toFirstChild&amp;AC=17</to>  
    </rule> 
<!-- 跳转到子节点 结束 -->
    <rule>  
        <from>^/assistant$</from>  
        <to>/clerk.do?method=index</to>  
    </rule> 
	<rule>  
        <from>^/order$</from>  
        <to>/orderInfo.do?method=index</to>  
    </rule> 
	<rule>  
        <from>^/bill$</from>  
        <to>/billInfo.do?method=index</to>  
    </rule> 
     
</urlrewrite>  