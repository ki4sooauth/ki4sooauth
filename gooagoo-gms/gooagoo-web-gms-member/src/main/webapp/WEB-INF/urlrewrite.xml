<?xml version="1.0" encoding="utf-8"?>  
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 4.0//EN"  
        "http://tuckey.org/res/dtds/urlrewrite4.0.dtd">  
<urlrewrite>
<!-- 跳转到子节点 开始-->
    <rule>  
        <from>^/$</from>  
        <to>/common.do?method=toFirstChild&amp;AC=2</to>  
    </rule> 
    <rule>  
        <from>^/index$</from>  
        <to>/common.do?method=toFirstChild&amp;AC=2</to>  
    </rule> 
<!-- 跳转到子节点 结束 -->
