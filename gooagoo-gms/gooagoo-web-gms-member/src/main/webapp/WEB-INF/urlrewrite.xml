        <to>/upAndDown.do?method=import</to>  
    </rule>
    <rule>  
        <from>^/importMemberInfo$</from>  
        <to>/upAndDown.do?method=importMemberInfo</to>  
    </rule>
    <!-- 会员卡管理 -->
    <rule>  
        <from>^/card$</from>  
        <to>/memberCard.do?method=index</to>  
    </rule>
    <rule>  
        <from>^/cardManage$</from>  
        <to>/memberCard.do?method=index</to>  
