    <rule>  
        <from>^/delete$</from>  
        <to>/rule.do?method=delete</to>  
    </rule>
    
    <rule>  
        <from>^/status$</from>  
        <to>/userState.do?method=index</to>  
    </rule>
    <rule>  
        <from>^/record$</from>  
        <to>/userRecord.do?method=index</to>  
    </rule>
    
</urlrewrite>  