<?xml version="1.0" encoding="utf-8"?>  
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 4.0//EN"  
        "http://tuckey.org/res/dtds/urlrewrite4.0.dtd">
<urlrewrite>
<!-- 跳转到子节点 开始-->
    <rule>  
        <from>^/$</from>  
        <to>/common.do?method=toFirstChild&amp;AC=15</to>  
    </rule> 
    <rule>  
        <from>^/index$</from>  
        <to>/common.do?method=toFirstChild&amp;AC=15</to>  
    </rule> 
<!-- 跳转到子节点 结束 -->
	<rule>
		<from>^/goods$</from>
		<to>/goods.do?method=index</to>
	</rule>
	<rule>
		<from>^/marketing$</from>
		<to>/goodsMarketing.do?method=index</to>
	</rule>
	<rule>
		<from>^/add$</from>
		<to>/goods.do?method=addGoods</to>
	</rule>
	<rule>
		<from>^/search$</from>
		<to>/goods.do?method=index</to>
	</rule>
	<rule>
		<from>^/category$</from>
		<to>/goodsCategory.do?method=index</to>
	</rule>
    <rule>
		<from>^/brand$</from>
		<to>/goodsBrand.do?method=index</to>
	</rule>
    <rule>
		<from>^/special$</from>
		<to>/feature.do?method=index</to>
	</rule>
</urlrewrite>  