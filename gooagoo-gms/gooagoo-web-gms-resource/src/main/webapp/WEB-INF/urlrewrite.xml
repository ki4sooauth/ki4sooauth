<?xml version="1.0" encoding="utf-8"?>  
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 4.0//EN"  
        "http://tuckey.org/res/dtds/urlrewrite4.0.dtd">  
<urlrewrite>  
<!-- 跳转到子节点 开始-->
    <rule>  
        <from>^/$</from>  
        <to>/common.do?method=toFirstChild&amp;AC=18</to>  
    </rule> 
<!-- 跳转到子节点 结束 -->
    <!-- 广告资源-->
     <rule>  
        <from>^/adResource</from>  
        <to>/auction.do?method=toIndex</to>  
    </rule> 
     <!-- 历史竞拍记录 -->
    <rule>  
        <from>^/bidHistory</from>  
        <to>/auctionRecord.do?method=toAllBidRList</to>  
    </rule>
</urlrewrite>  