<?xml version="1.0" encoding="utf-8"?>  
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 4.0//EN"  
        "http://tuckey.org/res/dtds/urlrewrite4.0.dtd">  
<urlrewrite>  
    <rule>  
        <from>^/$</from>  
        <to>/activity.do?method=index</to>  
    </rule>  
    <rule>  
        <from>^/index$</from>  
        <to>/activity.do?method=index</to>  
    </rule>  
  
    <rule>  
        <from>^/event$</from>  
        <to>/event.do?method=index</to>  
    </rule> 
    <rule>  
        <from>^/rule$</from>  
        <to>/rule.do?method=index</to>  
    </rule> 
    <!-- 活动内容列表 -->
    <rule>  
        <from>^/content$</from>  
        <to>/activityCont.do?method=index</to>  
    </rule> 
    <!-- 活动内容管理列表 -->
    <rule>  
        <from>^/activeContent$</from>  
        <to>/activityCont.do?method=indexPage</to>  
    </rule> 

        
    <!-- 短信管理 -->
    <rule>  
        <from>^/message$</from>  
        <to>/message.do?method=index</to>  
    </rule>

    <!-- 吆喝管理 -->
    <rule>  
        <from>^/cryout$</from>  
        <to>/cryout.do?method=index</to>  
    </rule>
    <rule>  
        <from>^/list$</from>  
        <to>/cryout.do?method=list</to>  
    </rule>
    <rule>  
        <from>^/save$</from>  
        <to>/cryout.do?method=save</to>  
    </rule>
    <rule>  
        <from>^/reSave$</from>  
        <to>/cryout.do?method=reSave</to>  
    </rule>
    <rule>  
        <from>^/loadNewCryout$</from>  
        <to>/cryout.do?method=loadNewCryout</to>  
    </rule>
    <rule>  
        <from>^/members$</from>  
        <to>/cryout.do?method=members</to>  
    </rule>
    <rule>  
        <from>^/releaseLog$</from>  
        <to>/cryout.do?method=releaseLog</to>  
    </rule>
    <!-- 通知管理 -->
    <rule>  
        <from>^/notice$</from>  
        <to>/notice.do?method=index</to>  
    </rule>
    <rule>  
        <from>^/sendList$</from>  
        <to>/notice.do?method=sendList</to>  
    </rule>
    <rule>  
        <from>^/receiveList$</from>  
        <to>/notice.do?method=receiveList</to>  
    </rule>
    <rule>  
        <from>^/add$</from>  
        <to>/notice.do?method=add</to>  
    </rule>
    <rule>  
        <from>^/save$</from>  
        <to>/notice.do?method=save</to>  
    </rule>
    <rule>  
        <from>^/delete$</from>  
        <to>/notice.do?method=delete</to>  
    </rule>
    <rule>  
        <from>^/read$</from>  
        <to>/notice.do?method=read</to>  
    </rule>
    <rule>  
        <from>^/sendDetail$</from>  
        <to>/notice.do?method=read</to>  
    </rule>
    <rule>  
        <from>^/receivedDetail$</from>  
        <to>/notice.do?method=receivedDetail</to>  
    </rule>
    <rule>  
        <from>^/members$</from>  
        <to>/notice.do?method=members</to>  
    </rule>
    <rule>  
        <from>^/releaseLog$</from>  
        <to>/notice.do?method=releaseLog</to>  
    </rule>
    <!-- relate -->
    <rule>  
        <from>^/getActivity$</from>  
        <to>/relate.do?method=getActivity</to>  
    </rule>
    <rule>  
        <from>^/pageActivity$</from>  
        <to>/relate.do?method=pageActivity</to>  
    </rule>
    <rule>  
        <from>^/getGoodsInfo$</from>  
        <to>/relate.do?method=getGoodsInfo</to>  
    </rule>
    <rule>  
        <from>^/pageGoodsInfo$</from>  
        <to>/relate.do?method=pageGoodsInfo</to>  
    </rule>
    <rule>  
        <from>^/getCoupon$</from>  
        <to>/relate.do?method=getCoupon</to>  
    </rule>
    <rule>  
        <from>^/pageCoupon$</from>  
        <to>/relate.do?method=pageCoupon</to>  
    </rule>

    
</urlrewrite>  