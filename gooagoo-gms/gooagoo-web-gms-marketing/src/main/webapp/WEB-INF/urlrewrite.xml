<?xml version="1.0" encoding="utf-8"?>  
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 4.0//EN"  
        "http://tuckey.org/res/dtds/urlrewrite4.0.dtd">  
<urlrewrite>  
<!-- 跳转到子节点 开始-->
    <rule>  
        <from>^/$</from>  
        <to>/common.do?method=toFirstChild&amp;AC=13</to>  
    </rule> 
    <rule>  
        <from>^/index$</from>  
        <to>/common.do?method=toFirstChild&amp;AC=13</to>  
    </rule> 
<!-- 跳转到子节点 结束 -->
    	  <!-- 优惠凭证管理 --> 
    <rule>  
        <from>^/coupon$</from>  
        <to>/coupon.do?method=index</to>  
    </rule> 

        <!-- 积分兑换管理列表 -->
    <rule>  
        <from>^/integral$</from>  
        <to>/integral.do?method=index</to>  
    </rule>  
    
        <!-- 精品推荐管理列表 -->
    <rule>  
        <from>^/jingpin$</from>  
        <to>/jingpin.do?method=index</to>  
    </rule> 
        <!-- 积分发放 -->
    <rule>  
        <from>^/integralDeliver$</from>  
        <to>/integralDeliver.do?method=index</to>  
    </rule> 

        <!-- 活动内容管理列表 -->
    <rule>  
        <from>^/activeContent$</from>  
        <to>/activityCont.do?method=indexPage</to>  
    </rule> 
    <!-- 吆喝 -->
    <rule>  
        <from>^/cryout$</from>  
        <to>/cryout.do?method=index</to>  
    </rule> 
    <!-- 通知 -->
    <rule>  
        <from>^/notice$</from>  
        <to>/notice.do?method=index</to>  
    </rule> 
    
</urlrewrite>  