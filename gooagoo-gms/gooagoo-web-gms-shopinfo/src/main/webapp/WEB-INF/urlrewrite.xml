<?xml version="1.0" encoding="utf-8"?>  
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 4.0//EN"  
        "http://tuckey.org/res/dtds/urlrewrite4.0.dtd">  
<urlrewrite>  
<!-- 跳转到子节点 开始-->
    <rule>  
        <from>^/$</from>  
        <to>/common.do?method=toFirstChild&amp;AC=14</to>  
    </rule> 
    <rule>  
        <from>^/index$</from>  
        <to>/common.do?method=toFirstChild&amp;AC=14</to>  
    </rule> 
    <rule>  
        <from>^/shop$</from>  
        <to>/common.do?method=toFirstChild&amp;AC=1401</to>  
    </rule>
    <rule>  
        <from>^/store$</from>  
        <to>/common.do?method=toFirstChild&amp;AC=1402</to>  
    </rule>
    <rule>  
        <from>^/device$</from>  
        <to>/common.do?method=toFirstChild&amp;AC=1403</to>  
    </rule>
    <rule>  
        <from>^/code$</from>  
        <to>/common.do?method=toFirstChild&amp;AC=1404</to>  
    </rule>
<!-- 跳转到子节点 结束 -->
     <rule>  
        <from>^/tcode$</from>  
        <to>/qrCode.do?method=index</to>  
    </rule>
     <rule>  
        <from>^/gcode$</from>  
        <to>/qrCode.do?method=gIndex</to>  
    </rule>
     <rule>  
        <from>^/ccode$</from>  
        <to>/qrCode.do?method=cIndex</to>  
    </rule>
     <rule>  
        <from>^/acode$</from>  
        <to>/qrCode.do?method=aIndex</to>  
    </rule>
     <rule>  
        <from>^/entity$</from>  
        <to>/shopEntity.do?method=index</to>  
    </rule>
    <rule>  
        <from>^/table$</from>  
        <to>/tableInfo.do?method=index</to>  
    </rule>      
    
    <rule>  
        <from>^/user$</from>  
        <to>/shopUser.do?method=index</to>  
    </rule>   
    <rule>  
        <from>^/position$</from>  
        <to>/position.do?method=index</to>  
    </rule> 
    <rule>  
        <from>^/passport$</from>  
        <to>/password.do?method=index</to>  
    </rule> 
    <rule>  
        <from>^/bindPhone$</from>  
        <to>/phone.do?method=index</to>  
    </rule> 
    <rule>  
        <from>^/wifi$</from>  
        <to>/wifi.do?method=index</to>  
    </rule> 
     <rule>  
        <from>^/transponder$</from>  
        <to>/transpcInfo.do?method=index</to>  
    </rule> 
      <rule>  
        <from>^/lid$</from>  
        <to>/shopLidDetail.do?method=index</to>  
    </rule> 
     <rule>  
        <from>^/logo$</from>  
        <to>/headAction.do?method=getHeadPic</to>  
    </rule> 
    
    <rule>  
        <from>^/baseinfo$</from>  
        <to>/shopInfo.do?method=getShopInfo</to>  
    </rule> 
     <rule>  
        <from>^/auth$</from>  
        <to>/shopRole.do?method=index</to>  
    </rule> 
     <rule>  
        <from>^/wifisensor$</from>  
        <to>/wifisensor.do?method=index</to>  
    </rule> 
     <rule>  
        <from>^/tableType$</from>  
        <to>/shopTableType.do?method=index</to>  
    </rule> 
</urlrewrite>  