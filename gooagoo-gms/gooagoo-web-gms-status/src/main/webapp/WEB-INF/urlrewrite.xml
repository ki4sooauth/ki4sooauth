<?xml version="1.0" encoding="utf-8"?>  
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 4.0//EN"  
        "http://tuckey.org/res/dtds/urlrewrite4.0.dtd">  
<urlrewrite>
<!-- 跳转到子节点 开始-->
    <rule>  
        <from>^/$</from>  
        <to>/common.do?method=toFirstChild&amp;AC=11</to>  
    </rule> 
    <rule>  
        <from>^/index$</from>  
        <to>/common.do?method=toFirstChild&amp;AC=11</to>  
    </rule> 
<!-- 跳转到子节点 结束 -->
	<!-- 会员管理 -->
    <rule>  
        <from>^/entity$</from>  
        <to>/entity.do?method=index</to>  
    </rule>
    <rule>  
        <from>^/activity$</from>  
        <to>/activity.do?method=index</to>  
    </rule>
    <rule>  
        <from>^/goods$</from>  
        <to>/goods.do?method=index</to>  
    </rule>
    <rule>  
        <from>^/category$</from>  
        <to>/category.do?method=index</to>  
    </rule>
    <rule>  
        <from>^/brand$</from>  
        <to>/brand.do?method=index</to>  
    </rule>
    <rule>  
        <from>^/coupon$</from>  
        <to>/coupon.do?method=index</to>  
    </rule>
    <rule>  
        <from>^/tools$</from>  
        <to>/tools.do?method=index</to>  
    </rule>
    <rule>  
        <from>^/table$</from>  
        <to>/table.do?method=index</to>  
    </rule>
    <rule>  
        <from>^/cryout$</from>  
        <to>/cryout.do?method=index</to>  
    </rule>
    <rule>  
        <from>^/notice$</from>  
        <to>/notice.do?method=index</to>  
    </rule>
</urlrewrite>  